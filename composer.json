{
    "name": "robbrazier/piwik",
    "description": "Piwik Package for Laravel",
    "license": "MIT",
    "version": "4.0.0",
    "keywords": ["laravel","piwik"],
    "authors": [
        {
            "name": "Rob Brazier"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "~6.2 | ^7.0.1",
        "lightools/xml": "^1.0",
        "illuminate/support": ">=5.2 | <= 5.5"
    },
    "require-dev": {
        "orchestra/testbench": "3.2.*",
        "phpunit/phpunit": "5.7.*",
        "phpunit/phpcov": "3.1.*",
        "phpspec/prophecy": "^1.7"
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpdbg -qrr ./vendor/bin/phpunit",
        "changelog": "sh .semaphore/changelog",
        "version:major": "bash .semaphore/increment-version major | .semaphore/update-versions",
        "version:minor": "bash .semaphore/increment-version minor | .semaphore/update-versions",
        "version:patch": "bash .semaphore/increment-version patch | .semaphore/update-versions"
    },
    "autoload": {
        "psr-0": {
            "RobBrazier\\Piwik": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RobBrazier\\Piwik\\PiwikServiceProvider"
            ],
            "aliases": {
                "Piwik": "RobBrazier\\Piwik\\Facades\\Piwik"
            }
        }
    }
}
